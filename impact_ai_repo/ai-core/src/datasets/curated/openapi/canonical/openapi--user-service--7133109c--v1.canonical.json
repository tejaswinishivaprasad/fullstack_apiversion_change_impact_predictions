              }
            }
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "InstanceId",
                  "InstanceOSUser",
                  "SSHPublicKey"
                ],
                "title": "SendSSHPublicKeyRequest",
                "properties": {
                  "InstanceId": {
                    "allOf": [
                      {
                        "type": "string",
                        "pattern": "^i-[a-f0-9]+$",
                        "minLength": 10,
                        "maxLength": 32
                      },
                      {
                        "description": "The ID of the EC2 instance."
                      }
                    ],
                    "_polymorphic_marker": {
                      "allOf": true
                    }
                  },
                  "InstanceOSUser": {
                    "allOf": [
                      {
                        "type": "string",
                        "pattern": "^[A-Za-z_][A-Za-z0-9\\@\\._-]{0,30}[A-Za-z0-9\\$_-]?$",
                        "minLength": 1,
                        "maxLength": 32
                      },
                      {
                        "description": "The OS user on the EC2 instance for whom the key can be used to authenticate."
                      }
                    ],
                    "_polymorphic_marker": {
                      "allOf": true
                    }
                  },
                  "SSHPublicKey": {
                    "allOf": [
                      {
                        "type": "string",
                        "minLength": 80,
                        "maxLength": 4096
                      },
                      {
                        "description": "The public key material. To use the public key, you must have the matching private key."
                      }
                    ],
                    "_polymorphic_marker": {
                      "allOf": true
                    }
                  },
                  "AvailabilityZone": {
                    "allOf": [
                      {
                        "type": "string",
                        "pattern": "^(\\w+-){2,3}\\d+\\w+$",
                        "minLength": 6,
                        "maxLength": 32
                      },
                      {
                        "description": "The Availability Zone in which the EC2 instance was launched."
                      }
                    ],
                    "_polymorphic_marker": {
                      "allOf": true
                    }
                  }
                }
              }
            }
          }
        },
        "parameters": [
